{
  "name": "rcm/redirect-editor",
  "description": "Rcm Plugin module to edit all parts of a redirect.",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "rcm",
    "module",
    "zf2",
    "redirect"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/bjanish/rcm-redirect-editor",
  "authors": [
    {
      "name": "Brian Janish",
      "email": "bjanish@relivinc.com",
      "homepage": "https://github.com/reliv"
    }
  ],
  "require": {
    "php": ">=5.5",
    "psr/container": "^1",
    "psr/http-message": "^1",
    "rcm/api-lib": "~0.10",
    "rcm/core": "~1.20",
    "rcm/user": "^1",
    "zendframework/zend-diactoros": "^1",
    "zendframework/zend-filter": "2.*",
    "zendframework/zend-http": "2.*",
    "zendframework/zend-inputfilter": "2.*",
    "zendframework/zend-mvc": "2.*",
    "zendframework/zend-view": "2.*",
    "zendframework/zend-validator": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "RcmRedirectEditor\\": "src/"
    }
  },
  "suggest": {
    "bower-asset/angular": "Required for JS",
    "bower-asset/bootstrap": "Client uses bootstrap",
    "bower-asset/rcm-loading": "Required for JS",
    "rcm/admin": "Allows RCM access to redirect links",
    "rcm/core-js": "Lots of JS libs"
  }
}
